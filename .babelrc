{
  "env": {
    "development": {
      "presets": ["react-app"]
    },
    "production": {
      "presets": ["react-app"]
    },
    "publish": {
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
          "@babel/plugin-proposal-object-rest-spread",
          {
            "useBuiltIns": true
          }
        ],
        "@babel/plugin-transform-runtime"
      ],
      "presets": [
        ["@babel/preset-env",
          {
            "targets": {
              "browsers": [
                "last 2 versions",
                "ie >= 9"
              ]
            },
            "loose": true,
            "useBuiltIns": "entry",
            "modules": false
          }
        ],
        "@babel/preset-react"
      ]
    },
    "test": {
      "plugins": ["transform-react-remove-prop-types"],
      "presets": ["react-app"]
    }
  }
}
